{
  "name": "mehdi.cafe",
  "version": "3.8.2",
  "private": false,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "cp tsconfig.en.json tsconfig.json && next dev",
    "build": "cp tsconfig.en.json tsconfig.json && next build",
    "build:fr": "cp tsconfig.fr.json tsconfig.json && next build",
    "start": "cp tsconfig.en.json tsconfig.json && next start",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fullpage/react-fullpage": "^0.1.40",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.18",
    "@types/node": "20.12.12",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.18",
    "@types/styled-components": "^5.1.34",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "npm run lint:fix"
    ]
  }
}
